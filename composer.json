{
    "name": "jiny/frontmatter",
    "description": "frontMatter for JinyPHP",
    "keywords": [
        "jinyPHP", "Laravel", "framwork"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\FrontMatter\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\FrontMatter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\FrontMatter\\JinyFrontMatterServiceProvider"
            ]
        }
    }
}
